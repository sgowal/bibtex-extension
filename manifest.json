{
  "name": "BibTeX Maker",
  "description": "Creates a BibTeX entry corresponding to a page.",
  "version": "1.0",

  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "tabs",
    "storage",
    "https://arxiv.org/abs/", "https://arxiv.org/pdf/",
    "https://www.medrxiv.org/content/",
    "https://www.biorxiv.org/content/",
    "https://openreview.net/forum?id=*", "https://openreview.net/pdf?id=*",
    "https://papers.nips.cc/paper/",
    "http://www.roboticsproceedings.org/",
    "https://distill.pub/",
    "https://www.nature.com/articles/",
    "https://science.sciencemag.org/content/",
    "https://ieeexplore.ieee.org/document/"
  ],

  "manifest_version": 2
}
